train_net: "mnist/custom_auto_train.prototxt"
test_net: "mnist/custom_auto_test.prototxt"
test_iter: 100
test_interval: 500
base_lr: 0.01
max_iter: 100
lr_policy: "inv"
gamma: 0.0001
power: 0.75
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "output/mnist"
solver_mode: GPU
type: "SGD"
